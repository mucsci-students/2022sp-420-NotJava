<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="UMLEditor.Views.TestingPanel"
             HorizontalAlignment="Stretch"
             VerticalAlignment="Stretch"
             Background="Red">
    <StackPanel>
        <Label Content="Enter name:"></Label>
        <TextBox Name="NameField" Watermark="Enter name here"></TextBox>
        <Label Content="Enter type:"></Label>
        <TextBox Name="TypeField" Watermark="Enter type here"></TextBox>
    </StackPanel>
</UserControl>
