<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:UMLEditor"
             x:Class="UMLEditor.App"
             Name="UML Editor">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme Mode="Dark"/>
    </Application.Styles>

    <NativeMenu.Menu>
        <NativeMenu>
            <NativeMenuItem Header="Exit" Gesture="cmd+q" Click="NativeMenuItem_OnClick" />
        </NativeMenu>
    </NativeMenu.Menu>
    
</Application>
